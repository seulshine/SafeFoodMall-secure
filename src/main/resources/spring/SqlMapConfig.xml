<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.ssafy.vo.User" alias="user" />
		<typeAlias type="com.ssafy.vo.Food" alias="food" />
		<typeAlias type="com.ssafy.vo.History" alias="history" />
	</typeAliases>

	<mappers>
		<mapper resource="spring/usermapper.xml" />
		<mapper resource="spring/foodmapper.xml" />
		<mapper resource="spring/historymapper.xml" />
	</mappers>
</configuration>


















